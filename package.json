{
	"name": "totp-generator-ts",
	"version": "1.0.2",
	"description": "Generate time-based one-time passwords.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"keywords": [
		"totp",
		"totp-token",
		"one-time password",
		"2fa",
		"totp-generator"
	],
	"scripts": {
		"dev": "ts-node src/index.ts",
		"build": "tsup src/index.ts --dts --format cjs,esm",
		"test": "vitest run",
		"lint": "eslint src --ext .ts && tsc",
		"format": "prettier --write .",
		"prepublish": "yarn test && yarn lint && yarn format && yarn build"
	},
	"repository": "https://github.com/defnot001/totp-generator-ts.git",
	"bugs": "https://github.com/defnot001/totp-generator-ts/issues.git",
	"author": "defnot001",
	"license": "MIT",
	"dependencies": {
		"jssha": "^3.3.0",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"eslint": "^8.33.0",
		"prettier": "^2.8.3",
		"tsconfig-paths": "^4.1.2",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4",
		"vitest": "^0.28.3"
	}
}
